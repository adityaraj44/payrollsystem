  <br />

  <label for="phone">Phone No.</label><br />
  <input type="number" id="phone" name="phone" /><br /><br />

  <label for="age">Age:</label><br />
  <input type="text" id="age" name="age" /><br /><br />

  <label for="city">CITY:</label><br />
  <input type="text" id="city" name="city" /><br /><br />

  <label for="state">STATE:</label><br />
  <input type="text" id="state" name="state" /><br /><br />

  <label for="pan">PAN NO.:</label><br />
  <input type="text" id="pan" name="pan" /><br /><br />

  <label for="adhar">ADHAR NO.:</label><br />
  <input type="number" id="adhar" name="adhar" /><br /><br />

  <label for="esic">ESIC NO.:</label><br />
  <input type="text" id="esic" name="esic" /><br /><br />

  <label for="pf">PF NO.:</label><br />
  <input type="text" id="pf" name="pf" /><br /><br />

  <button type="submit">Register</button>
</form>
